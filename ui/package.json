{
  "name": "dataui",
  "version": "1.0.0",
  "description": "immotep UI",
  "homepage": "/immotep/",
  "scripts": {
    "start:dev": "webpack serve --host 0.0.0.0 --mode development",
    "start": "webpack serve --host 0.0.0.0 --mode production",
    "build:dist": "webpack --mode production",
    "build:watch": "webpack --mode production --watch",
    "build": "npm i --no-optional --no-shrinkwrap --no-package-lock && npm run build:dist",
    "doc": "jsdoc -R README.md -P package.json -d doc -r -c jsdoc.config.json frontend",
    "test": "jest --passWithNoTests",
    "test:watch": "npm test -- --watch",
    "coverage": "jest --passWithNoTests --coverage",
    "build:analyze": "npx webpack --profile --mode production --json > stats.json && npx webpack-bundle-analyzer stats.json"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.14.5",
    "@babel/core": "^7.14.6",
    "@babel/eslint-parser": "^7.14.7",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/preset-env": "^7.14.7",
    "@babel/preset-react": "^7.14.5",
    "@typescript-eslint/eslint-plugin": "^4.28.1",
    "@typescript-eslint/parser": "^4.28.1",
    "babel-jest": "^27.0.6",
    "babel-loader": "^8.2.2",
    "clean-webpack-plugin": "^4.0.0-alpha.0",
    "copy-webpack-plugin": "^9.0.1",
    "copyfiles": "^2.4.1",
    "css-loader": "^6.2.0",
    "eslint": "^7.29.0",
    "eslint-config-react-app": "^6.0.0",
    "eslint-plugin-import": "2.23.4",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-react": "7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-webpack-plugin": "3.0.1",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.3.2",
    "jest": "^27.0.6",
    "jest-html-reporter": "^3.4.1",
    "jsdoc": "3.6.7",
    "react-test-renderer": "^17.0.2",
    "rimraf": "^3.0.2",
    "style-loader": "^3.0.0",
    "webpack": "^5.41.0",
    "webpack-cli": "^4.7.2",
    "webpack-dev-server": "^3.11.2"
  },
  "dependencies": {
    "@material-ui/core": "^4.11.4",
    "@material-ui/icons": "^4.11.2",
    "@material-ui/lab": "^4.0.0-alpha.58",
    "@reduxjs/toolkit": "^1.5.1",
    "@types/leaflet": "^1.7.4",
    "leaflet": "^1.7.1",
    "react-leaflet": "^3.2.0",
    "axios": "^0.21.1",
    "fontsource-roboto": "^4.0.0",
    "i18next": "^20.3.2",
    "i18next-browser-languagedetector": "^6.1.2",
    "i18next-http-backend": "^1.2.2",
    "material-ui-flags": "^1.2.4",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-i18next": "^11.11.0",
    "react-router-dom": "^5.2.0",
    "react-redux": "^7.2.3",
    "redux-logger": "^3.0.6",
    "redux-persist": "^6.0.0",
    "regenerator-runtime": "^0.13.7",
    "yargs": "^17.0.1"
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ],
    "plugins": [
      "@babel/plugin-syntax-dynamic-import"
    ]
  },
  "jest": {
    "globals": {
      "__JEST__": true
    },
    "moduleNameMapper": {
      "\\.css$": "identity-obj-proxy",
      "fontsource-roboto$": "identity-obj-proxy",
      "\\-worker.js$": "<rootDir>/__mocks__/webpack-worker.js",
      "jspdf.*\\.js": "<rootDir>/__mocks__/webpack-worker.js"
    },
    "reporters": [
      "default",
      [
        "./node_modules/jest-html-reporter",
        {
          "pageTitle": "Test Report"
        }
      ]
    ],
    "collectCoverage": false,
    "collectCoverageFrom": [
      "**/src/**/*.{js,jsx}"
    ],
    "coverageDirectory": "coverage",
    "coverageReporters": [
      "lcov",
      "html",
      "text",
      "text-summary"
    ]
  },
  "eslintConfig": {
    "extends": "react-app",
    "plugins": [
      "compat"
    ],
    "rules": {
      "compat/compat": "warn"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
