{
  "name": "dataui",
  "version": "1.0.0",
  "description": "immotep UI",
  "homepage": "/immotep/",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "doc": "jsdoc -R README.md -P package.json -d doc -r -c jsdoc.config.json frontend",
    "test": "jest --passWithNoTests",
    "test:watch": "npm test -- --watch",
    "coverage": "jest --passWithNoTests --coverage"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/node": "^24.8.1",
    "@vitejs/plugin-react": "^4.7.0",
    "oxlint": "^1.16.0",
    "rimraf": "^6.0.1",
    "vite": "^6.4.0",
    "vite-plugin-oxlint": "^1.4.0"
  },
  "dependencies": {
    "@emotion/react": "11.14.0",
    "@emotion/styled": "11.14.1",
    "@fontsource/roboto": "5.2.8",
    "@mui/icons-material": "7.3.4",
    "@mui/lab": "^7.0.1-beta.18",
    "@mui/material": "7.3.4",
    "@reduxjs/toolkit": "2.9.1",
    "@tanstack/react-query": "^5.90.5",
    "@tanstack/react-query-devtools": "^5.90.2",
    "@types/leaflet": "^1.9.21",
    "axios": "^1.12.2",
    "i18next": "25.6.0",
    "i18next-browser-languagedetector": "8.2.0",
    "i18next-http-backend": "3.0.2",
    "leaflet": "^1.9.4",
    "loglevel": "1.9.2",
    "material-ui-flags": "^1.2.4",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-i18next": "16.1.0",
    "react-leaflet": "^5.0.0",
    "react-leaflet-draw": "^0.21.0",
    "react-redux": "^9.2.0",
    "react-router-dom": "7.9.4",
    "redux-logger": "^3.0.6",
    "regenerator-runtime": "^0.14.1",
    "yargs": "^17.7.2"
  },
  "jest": {
    "globals": {
      "__JEST__": true
    },
    "moduleNameMapper": {
      "\\.css$": "identity-obj-proxy",
      "fontsource-roboto$": "identity-obj-proxy",
      "\\-worker.js$": "<rootDir>/__mocks__/webpack-worker.js",
      "jspdf.*\\.js": "<rootDir>/__mocks__/webpack-worker.js"
    },
    "reporters": [
      "default",
      [
        "./node_modules/jest-html-reporter",
        {
          "pageTitle": "Test Report"
        }
      ]
    ],
    "collectCoverage": false,
    "collectCoverageFrom": [
      "**/src/**/*.{js,jsx}"
    ],
    "coverageDirectory": "coverage",
    "coverageReporters": [
      "lcov",
      "html",
      "text",
      "text-summary"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
