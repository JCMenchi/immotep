{
  "name": "dataui",
  "version": "1.0.0",
  "description": "immotep UI",
  "homepage": "/immotep/",
  "scripts": {
    "start:dev": "webpack serve --host 0.0.0.0 --mode development",
    "start": "webpack serve --host 0.0.0.0 --mode production",
    "build:dist": "webpack --mode production",
    "build:watch": "webpack --mode production --watch",
    "build": "npm i --omit=optional --no-package-lock && npm run build:dist",
    "doc": "jsdoc -R README.md -P package.json -d doc -r -c jsdoc.config.json frontend",
    "test": "jest --passWithNoTests",
    "test:watch": "npm test -- --watch",
    "coverage": "jest --passWithNoTests --coverage",
    "build:analyze": "npx webpack --profile --mode production --json > stats.json && npx webpack-bundle-analyzer stats.json"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "7.25.9",
    "@babel/core": "7.26.0",
    "@babel/eslint-parser": "7.25.9",
    "@babel/plugin-syntax-dynamic-import": "7.8.3",
    "@babel/preset-env": "7.26.0",
    "@babel/preset-react": "7.25.9",
    "@typescript-eslint/eslint-plugin": "8.11.0",
    "@typescript-eslint/parser": "8.11.0",
    "babel-jest": "^29.7.0",
    "babel-loader": "9.2.1",
    "clean-webpack-plugin": "^4.0.0",
    "copy-webpack-plugin": "^12.0.2",
    "copyfiles": "^2.4.1",
    "css-loader": "^7.1.2",
    "eslint": "9.13.0",
    "eslint-plugin-compat": "6.0.1",
    "eslint-plugin-import": "2.31.0",
    "eslint-plugin-jsx-a11y": "6.10.2",
    "eslint-plugin-react": "7.37.2",
    "eslint-plugin-react-hooks": "5.0.0",
    "eslint-webpack-plugin": "4.2.0",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "5.6.3",
    "jest": "^29.7.0",
    "jest-html-reporter": "^3.10.2",
    "jsdoc": "4.0.4",
    "react-test-renderer": "^18.3.1",
    "rimraf": "^6.0.1",
    "style-loader": "^4.0.0",
    "webpack": "^5.94.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "5.1.0"
  },
  "dependencies": {
    "@emotion/react": "11.13.3",
    "@emotion/styled": "11.13.0",
    "@fontsource/roboto": "5.1.0",
    "@mui/icons-material": "6.1.5",
    "@mui/lab": "^6.0.0-alpha.14",
    "@mui/material": "6.1.5",
    "@reduxjs/toolkit": "2.3.0",
    "@types/leaflet": "^1.9.14",
    "axios": "1.7.7",
    "i18next": "23.16.4",
    "i18next-browser-languagedetector": "8.0.0",
    "i18next-http-backend": "2.6.2",
    "leaflet": "^1.9.4",
    "loglevel": "1.9.2",
    "material-ui-flags": "^1.2.4",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-i18next": "15.1.0",
    "react-leaflet": "^4.2.1",
    "react-leaflet-draw": "^0.20.4",
    "react-redux": "^9.1.2",
    "react-router-dom": "6.27.0",
    "redux-logger": "^3.0.6",
    "regenerator-runtime": "^0.14.1",
    "yargs": "^17.7.2"
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ],
    "plugins": [
      "@babel/plugin-syntax-dynamic-import"
    ]
  },
  "jest": {
    "globals": {
      "__JEST__": true
    },
    "moduleNameMapper": {
      "\\.css$": "identity-obj-proxy",
      "fontsource-roboto$": "identity-obj-proxy",
      "\\-worker.js$": "<rootDir>/__mocks__/webpack-worker.js",
      "jspdf.*\\.js": "<rootDir>/__mocks__/webpack-worker.js"
    },
    "reporters": [
      "default",
      [
        "./node_modules/jest-html-reporter",
        {
          "pageTitle": "Test Report"
        }
      ]
    ],
    "collectCoverage": false,
    "collectCoverageFrom": [
      "**/src/**/*.{js,jsx}"
    ],
    "coverageDirectory": "coverage",
    "coverageReporters": [
      "lcov",
      "html",
      "text",
      "text-summary"
    ]
  },
  "eslintConfig": {
    "extends": "react-app",
    "plugins": [
      "compat"
    ],
    "rules": {
      "compat/compat": "warn"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
