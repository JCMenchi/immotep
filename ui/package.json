{
  "name": "dataui",
  "version": "1.0.0",
  "description": "immotep UI",
  "homepage": "/immotep/",
  "scripts": {
    "start:dev": "webpack serve --host 0.0.0.0 --mode development",
    "start": "webpack serve --host 0.0.0.0 --mode production",
    "build:dist": "webpack --mode production",
    "build:watch": "webpack --mode production --watch",
    "build": "npm i --omit=optional --no-package-lock && npm run build:dist",
    "doc": "jsdoc -R README.md -P package.json -d doc -r -c jsdoc.config.json frontend",
    "test": "jest --passWithNoTests",
    "test:watch": "npm test -- --watch",
    "coverage": "jest --passWithNoTests --coverage",
    "build:analyze": "npx webpack --profile --mode production --json > stats.json && npx webpack-bundle-analyzer stats.json"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.24.8",
    "@babel/core": "^7.24.8",
    "@babel/eslint-parser": "^7.24.8",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/preset-env": "^7.24.8",
    "@babel/preset-react": "^7.24.7",
    "@typescript-eslint/eslint-plugin": "^7.16.1",
    "@typescript-eslint/parser": "^7.16.1",
    "babel-jest": "^29.7.0",
    "babel-loader": "^9.1.3",
    "clean-webpack-plugin": "^4.0.0",
    "copy-webpack-plugin": "^12.0.2",
    "copyfiles": "^2.4.1",
    "css-loader": "^7.1.2",
    "eslint": "^8.56.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-jsx-a11y": "6.9.0",
    "eslint-plugin-react": "7.35.0",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-webpack-plugin": "4.2.0",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.6.0",
    "jest": "^29.7.0",
    "jest-html-reporter": "^3.10.2",
    "jsdoc": "4.0.3",
    "react-test-renderer": "^18.3.1",
    "rimraf": "^6.0.1",
    "style-loader": "^4.0.0",
    "webpack": "^5.93.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.0.4"
  },
  "dependencies": {
    "@emotion/react": "^11.13.0",
    "@emotion/styled": "^11.13.0",
    "@fontsource/roboto": "^5.0.13",
    "@mui/icons-material": "^5.16.4",
    "@mui/lab": "^5.0.0-alpha.172",
    "@mui/material": "^5.16.4",
    "@reduxjs/toolkit": "^2.2.6",
    "@types/leaflet": "^1.9.12",
    "axios": "^1.7.2",
    "i18next": "^23.12.2",
    "i18next-browser-languagedetector": "^8.0.0",
    "i18next-http-backend": "^2.5.2",
    "leaflet": "^1.9.4",
    "loglevel": "^1.9.1",
    "material-ui-flags": "^1.2.4",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-i18next": "^15.0.0",
    "react-leaflet": "^4.2.1",
    "react-leaflet-draw": "^0.20.4",
    "react-redux": "^9.1.2",
    "react-router-dom": "^6.25.1",
    "redux-logger": "^3.0.6",
    "regenerator-runtime": "^0.14.1",
    "yargs": "^17.7.2"
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ],
    "plugins": [
      "@babel/plugin-syntax-dynamic-import"
    ]
  },
  "jest": {
    "globals": {
      "__JEST__": true
    },
    "moduleNameMapper": {
      "\\.css$": "identity-obj-proxy",
      "fontsource-roboto$": "identity-obj-proxy",
      "\\-worker.js$": "<rootDir>/__mocks__/webpack-worker.js",
      "jspdf.*\\.js": "<rootDir>/__mocks__/webpack-worker.js"
    },
    "reporters": [
      "default",
      [
        "./node_modules/jest-html-reporter",
        {
          "pageTitle": "Test Report"
        }
      ]
    ],
    "collectCoverage": false,
    "collectCoverageFrom": [
      "**/src/**/*.{js,jsx}"
    ],
    "coverageDirectory": "coverage",
    "coverageReporters": [
      "lcov",
      "html",
      "text",
      "text-summary"
    ]
  },
  "eslintConfig": {
    "extends": "react-app",
    "plugins": [
      "compat"
    ],
    "rules": {
      "compat/compat": "warn"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
